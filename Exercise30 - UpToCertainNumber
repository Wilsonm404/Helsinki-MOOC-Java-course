
import java.util.Scanner;


public class UpToCertainNumber {

    public static void main(String[] args) {
        Scanner reader = new Scanner(System.in);
        
        // Write your code here
        int incrementingInteger = 1;
        int userUpperLimit;
        
        System.out.print("Up to what number? ");
        userUpperLimit = Integer.parseInt(reader.nextLine());
        
        while(incrementingInteger <= userUpperLimit)
            {
                System.out.println(incrementingInteger);
                incrementingInteger++;
            }
        
    }
}
